{
  "host": "${process.env.HOST || '127.0.0.1'}",
  "port": "${process.env.PORT || 3001}",
  "proxy": {
    "enabled": false
  },
  "cron": {
    "enabled": false
  },
  "admin": {
    "autoOpen": false
  },
  "webhooks": {
    "defaultHeaders": {
      "Authorization": "Bearer ${ process.env.WEBHOOK_TOKEN }"
    }
  }
}
